{
  "name": "mapt",
  "description": "A tool to automate validation scripting",
  "version": "0.0.1",
  "private": true,
  "author": "adamshurson",
  "license": "MIT",
  "homepage": "./",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adamshurson/mapt.git"
  },
  "main": "public/electron.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "dev": "concurrently \"cross-env BROWSER=none npm start\" \"wait-on http://localhost:3000 && electron .\"",
    "prod": "build --em.main=build/electron.js",
    "release": "build --em.main=build/electron.js --publish=always",
    "preprod": "npm run build",
    "prerelease": "npm run build"
  },
  "dependencies": {
    "@ashurson/pearl": "^1.0.6",
    "@fortawesome/fontawesome-svg-core": "^1.2.4",
    "@fortawesome/free-regular-svg-icons": "^5.3.1",
    "@fortawesome/free-solid-svg-icons": "^5.3.1",
    "@fortawesome/react-fontawesome": "^0.1.3",
    "animate.css": "^3.7.0",
    "chart.js": "^2.7.2",
    "cross-env": "^5.1.3",
    "csv": "^3.1.0",
    "csv-generate": "latest",
    "csv-parse": "latest",
    "electron-is-dev": "^0.3.0",
    "electron-log": "^2.2.14",
    "electron-updater": "^2.19.1",
    "fs-extra": "^7.0.0",
    "history": "latest",
    "node-notifier": "^5.2.1",
    "react": "^16.2.0",
    "react-addons-css-transition-group": "^15.6.2",
    "react-chart.js": "^1.0.0",
    "react-chartjs": "^1.2.0",
    "react-chartjs-2": "^2.7.4",
    "react-dom": "^16.2.0",
    "react-router-dom": "^4.3.1"
  },
  "devDependencies": {
    "autoprefixer": "^9.1.5",
    "concurrently": "3.5.1",
    "electron": "^2.0.9",
    "electron-builder": "^19.54.0",
    "gulp": "^3.9.1",
    "gulp-postcss": "^8.0.0",
    "prettier": "^1.10.2",
    "react-scripts": "^1.1.5",
    "tailwindcss": "^0.6.5",
    "wait-on": "^2.1.0"
  },
  "build": {
    "appId": "com.adamshurson.mapt",
    "files": [
      "build/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "publish": {
      "provider": "github"
    }
  }
}
